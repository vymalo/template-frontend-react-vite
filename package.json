    "@vymalo/ui-preset": "^1.0.7",
    "autoprefixer": "^10.4.20",
    "buffer": "^6.0.3",
    "commander": "^12.1.0",
    "cssnano": "^7.0.6",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-sonarjs": "^1.0.4",
    "eslint-plugin-storybook": "^0.11.2",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.3.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
